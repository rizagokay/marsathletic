
@{
    ViewBag.Title = "M-Port | Tanımlı Listeler";
}
@using MarsAhletic.WebUI.Models
@model ListViewModel

<div class="row">
    <div class="col-sm-12">
        <h4 class="page-title">Tanımlı Listeler</h4>
        <ol class="breadcrumb">
            <li>
                <a href="javascript:void(0);">Ayarlar</a>
            </li>
            <li class="active">
                Tanımlı Listeler
            </li>
        </ol>
        <p class="text-muted page-title-alt">İndirilebilecek excel listelerini buradan yükleyebilir ve düzenleyebilirsiniz.</p>
    </div>
</div>

<div class="row">
    <div class="col-sm-4">
        <form method="post" action="@Url.Action("PredefinedLists")" enctype="multipart/form-data" data-parsley-validate>
            @Html.AntiForgeryToken()
            @Html.EditorFor(model => Model.DefinedLists)
           
            <a href="@Url.Action("AddNewList")" class="btn btn-default" >Yeni Liste Ekle</a>
        </form>
    </div>
</div>

@section finaljscodes{
    <script src="@Url.Content("~/assets/plugins/bootstrap-filestyle/js/bootstrap-filestyle.min.js")" type="text/javascript"></script>
}